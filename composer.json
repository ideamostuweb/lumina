    "autoload": {
        "psr-4": {
            "Lumina\\": "/src",
            "Lumina\\Commands\\": "/src/Commands",
            "Lumina\\Database\\": "/src/Database",
            "Lumina\\Exception\\": "/src/Exception"
        }
    },
    "authors": [
        {
            "name": "IDEAMOS TU WEB Agencia Digital",
            "email": "hola@ideamostuweb.com",
            "homepage": "https://www.ideamostuweb.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "vlucas/phpdotenv": "^5.5",
        "monolog/monolog": "^3.4",
        "catfan/medoo": "^2.1",
        "symfony/console": "^6.3"
    }
}